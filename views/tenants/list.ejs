<!-- Wappler include head-page="layouts/main" fontawesome_5="cdn" bootstrap5="local" is="dmx-app" id="list" appConnect="local" components="{dmxBootstrap5TableGenerator:{}}" -->
<dmx-serverconnect id="get_tenants" url="/api/tenant/list"></dmx-serverconnect>

<div class="container">
  <%- await include('/partials/page_heading', Object.assign({}, locals, {heading:'Tenants'})) %>
  <div class="row">
    <div class="table-responsive">
      <table class="table table-striped table-hover">
        <thead>
          <tr>
            <th>Tenant</th>
            <th>Name</th>
            <th>Title</th>
            <th>Description</th>
          </tr>
        </thead>
        <tbody is="dmx-repeat" dmx-generator="bs5table" dmx-bind:repeat="get_tenants.data.tenants" id="tableRepeat1">
          <tr>
            <td dmx-text="tenant_id"></td>
            <td dmx-text="name"></td>
            <td dmx-text="title"></td>
            <td dmx-text="description"></td>
          </tr>
        </tbody>
      </table>
    </div>

  </div>
</div>
<meta name="ac:route" content="/tenants/list">